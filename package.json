    "electron": "1.8.4",
    "electron-builder": "20.8.1",
    "faker": "^4.1.0",
    "prettier": "^1.12.0",
    "react-app-rewire-babel-loader": "^0.1.1",
    "react-app-rewired": "1.4.0",
    "react-scripts": "1.1.4",
    "source-map-loader": "^0.2.3",
    "source-map-support": "^0.5.4",
