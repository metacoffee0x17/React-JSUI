    "files": [
      "build/**/*"
    ],
    "mac": {
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.png"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "icons/icon.png"
    },
    "extraMetadata": {
      "main": "build/electron.js"
    },
    "directories": {
      "buildResources": "assets"
    },
    "publish": {
      "provider": "github"
    }
